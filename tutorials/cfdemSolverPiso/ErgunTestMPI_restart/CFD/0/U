FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    wall
    {
        type            slip;
    }

    inlet
    {
        type            uniformFixedValue;
        uniformValue     table
        (
            (0.000 (0 0 0.002))
            (0.100 (0 0 0.020))
        );
    }

    outlet
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
